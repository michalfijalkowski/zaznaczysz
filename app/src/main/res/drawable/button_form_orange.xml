<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
        <corners android:radius="7dp" />
      	<gradient
          	android:startColor="#ffa366"
          	android:centerColor="#ffc299"
          	android:endColor="#ffa366"
          	android:angle="45"
          	android:type="linear" />
      	<stroke
          	android:width="1dp"
          	android:color="#ff944d" />
      	<padding
         	android:left="10dp"
          	android:top="10dp"
          	android:right="10dp"
          	android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
        <corners android:radius="7dp" />
      	<gradient
          	android:startColor="#ffc299"
          	android:centerColor="#ffd1b3"
          	android:endColor="#ffc299"
          	android:angle="45"
          	android:type="linear" />
      	<stroke
          	android:width="1dp"
          	android:color="#ffa366" />
      	<padding
         	android:left="10dp"
          	android:top="10dp"
          	android:right="10dp"
          	android:bottom="10dp" />
    </shape>
  </item>
</selector>